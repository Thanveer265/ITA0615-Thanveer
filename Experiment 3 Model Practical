import numpy as np
from sklearn.linear_model import LinearRegression, LogisticRegression
from sklearn.model_selection import train_test_split
from sklearn.metrics import mean_squared_error, accuracy_score
X = np.array([[1], [2], [3], [4], [5]])
y_linear = np.array([2, 4, 6, 8, 10])
y_logistic = np.array([0, 0, 1, 1, 1])
lin_reg = LinearRegression()
lin_reg.fit(X, y_linear)
y_pred_lin = lin_reg.predict(X)
log_reg = LogisticRegression()
log_reg.fit(X, y_logistic)
y_pred_log = log_reg.predict(X)
print("Linear Regression MSE:", mean_squared_error(y_linear, y_pred_lin))
print("Logistic Regression Accuracy:", accuracy_score(y_logistic, y_pred_log))
